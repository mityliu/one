{
  "name": "one",
  "version": "0.0.0",
  "main": "dist/index.html",
  "repository": "https://gitee.com/ecruos/one.git",
  "author": "Ecruos <ecruos@outlook.com>",
  "scripts": {
    "dev": "parcel index.html --open",
    "build": "parcel build index.html --public-url /one/ && cp ./favicon.ico ./dist/favicon.ico",
    "build:local": "parcel build index.html && cp ./favicon.ico ./dist/favicon.ico"
