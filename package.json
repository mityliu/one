    "copy-text-to-clipboard": "^2.1.1",
    "linkstate": "^1.1.1",
    "milligram": "^1.3.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "sass": "^1.23.7"
  }
}
